group "0.sim" {

  cmd "0.kuka_simulation" {
    exec = "bazel-bin/examples/kuka_iiwa_arm/kuka_simulation --urdf drake/manipulation/models/iiwa_description/urdf/dual_iiwa14_polytope_collision.urdf --novisualize_frames";
    host = "localhost";
  }

  cmd "1.iiwa_controller" {
    exec = "bazel-bin/examples/kuka_iiwa_arm/iiwa_controller --urdf drake/manipulation/models/iiwa_description/urdf/dual_iiwa14_polytope_collision.urdf";
    host = "localhost";
  }

  cmd "2.state_lcm_translator" {
    exec = "bazel-bin/examples/kuka_iiwa_arm/kuka_iiwa_state_translator --urdf manipulation/models/iiwa_description/urdf/dual_iiwa14_polytope_collision.urdf";
    host = "localhost";
  }
}

group "1.tools" {

  cmd "0.drake-visualizer" {
    exec = "bazel-bin/tools/drake_visualizer";
    host = "localhost";
  }

  cmd "1.director-ik-app" {
    exec = "directorPython drake/examples/kuka_iiwa_arm/director_ik_app.py --director_config drake/examples/kuka_iiwa_arm/director_config_dual.json";
    host = "localhost";
  }
}
