group "0.sim" {

  cmd "0.kuka_simulation" {
    exec = "bazel-bin/examples/kuka_iiwa_arm/kuka_simulation";
    host = "localhost";
  }

  cmd "1.kuka_plan_runner" {
    exec = "bazel-bin/examples/kuka_iiwa_arm/kuka_plan_runner";
    host = "localhost";
  }

  cmd "2.state_lcm_translator" {
    exec = "bazel-bin/examples/kuka_iiwa_arm/kuka_iiwa_state_translator";
    host = "localhost";
  }
}

group "1.tools" {

  cmd "0.drake-visualizer" {
    exec = "bazel-bin/tools/drake_visualizer";
    host = "localhost";
  }


  cmd "1.director-ik-app" {
    exec = "directorPython drake/examples/kuka_iiwa_arm/director_ik_app.py --director_config drake/examples/kuka_iiwa_arm/director_config.json";
    host = "localhost";
  }
}
